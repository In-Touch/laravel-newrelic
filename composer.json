{
    "name":              "intouch/laravel-newrelic",
    "type":              "library",
    "keywords":          ["newrelic", "new relic", "laravel"],
    "homepage":          "https://github.com/In-Touch/laravel-newrelic",
    "license":           "Apache 2.0",
    "description":       "Laravel NewRelic Integration",
    "authors":           [
        {
            "name":  "Patrick Leckey",
            "email": "pleckey@intouchinsight.com"
        }
    ],
    "require":           {
        "php":                ">=7.2",
        "illuminate/support": "7.*",
        "intouch/newrelic":   "^2.0"
    },
    "require-dev":       {
    },
    "autoload":          {
        "psr-0": {
            "Intouch\\LaravelNewrelic": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intouch\\LaravelNewrelic\\NewrelicServiceProvider"
            ],
            "aliases": {
                "Newrelic": "Intouch\\LaravelNewrelic\\Facades\\Newrelic"
            }
        }
    },
    "minimum-stability": "dev"
}
